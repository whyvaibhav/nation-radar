{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "variables": {
    "RAILWAY_STATIC_URL": "https://nation-radar.up.railway.app",
    "NODE_ENV": "production"
  }
}
